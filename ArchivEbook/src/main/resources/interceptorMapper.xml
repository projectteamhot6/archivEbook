<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="global.sesoc.archive.dao.InterCeptorMapper">
	<select id="test" parameterType="community" resultType="community">
	select
		*
	from
		community
	where
		id = #{id}
		and
		groupnum = #{groupnum}
	</select>
</mapper>
